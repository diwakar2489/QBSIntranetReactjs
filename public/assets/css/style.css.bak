/*------ Toggle Button Start -------*/

.btn-toggle {
	margin: 0;
	padding: 0;
	position: relative;
	border: none;
	height: 42px;
	width: 134px;
	border-radius: 1.5rem;
	color: #6b7381;
	background: #fff;
	border: 1px solid #c2c2c2 !important;
}
.btn-toggle:focus,
.btn-toggle.focus,
.btn-toggle:focus.active,
.btn-toggle.focus.active {
  outline: none;
  box-shadow: none;
}
button.btn-toggle:focus:not(:focus-visible) {
  outline: 0;
  box-shadow: none;
}
.btn-toggle:before,
.btn-toggle:after {
  line-height: 2.5rem;
  text-align: center;
  font-weight: 600;
  font-size: 14px;
  text-transform: uppercase;
  letter-spacing: 2px;
  position: absolute;
  bottom: 0;
  transition: opacity 0.25s;
}
.btn-toggle:before {
  content: 'Inactive';
  left: 10px;
}
.btn-toggle:after {
	content: 'Active';
	right: 24px;
	opacity: 0;
}
.btn-toggle > .handle {
	position: absolute;
	top: 3px;
	left: 94px;
	width: 34px;
	height: 34px;
	border-radius: 1.125rem;
	background: #fff;
	transition: left 0.25s;
	background-color: #bdbdbd;
}
.btn-toggle.active {
  transition: background-color 0.25s;
}
.btn-toggle.active > .handle {
  left: 4px;
  transition: left 0.25s;
	background-color: #00346B;
}
.btn-toggle.active:before {
  opacity: 0;
}
.btn-toggle.active:after {
  opacity: 1;
}
.btn-toggle.btn-secondary {
  color: #6b7381;
  background: #fff;
}
.btn-toggle.btn-secondary:before,
.btn-toggle.btn-secondary:after {
  color: #6b7381;
}
.btn-toggle.btn-secondary.active  {
  background-color: #fff;
}