	box-shadow: 0px 1px 2px rgba(50, 43, 95, 0.08);
	border-radius: 2px 2px 0px 0px;
	transform: translate(-76px, 28px);
	min-width: 110px;
	padding: 0;
}
.more_icon_blk .dropdown-menu a{
	font-weight: 400;
	font-size: 13.75px;
	line-height: 24px;
	color: #666666;
	border-bottom: 1px solid rgba(165, 165, 166, 0.4);
	padding: 6px 12px;
}
.more_icon_blk .dropdown-menu a:last-child{
	border-bottom: none;
}
.more_icon_blk .dropdown-menu a i{
	margin-right: 5px;
}
