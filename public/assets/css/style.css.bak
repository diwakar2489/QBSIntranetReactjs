}
.form-check-input:checked {
  background: #00346B;
	border: 1px solid #00346B;
	border-radius: 4.73924px;
}
.form-check-input[type="checkbox"] {
  border-radius: 2px;
}
.form-check-input:focus {
  outline: 0;
  box-shadow: none;
}
