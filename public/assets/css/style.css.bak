/*--- Login Start ----*/
.login_container {
	width: 100%;
	float: left;
	background: url(../images/bg.png);
	background-size: 100% auto;
}
.login_wrapper {
  background: rgba(0, 52, 107, 0.7);
  width: 100%;
  float: left;
  height: 100%;
  padding: 60px 0;
}
.login_blk {
  width: 500px;
  margin: 0 auto;
}
.logo_blk1 {  
	text-align: center;
	margin-bottom: 30px;
}
.logo_blk1 img {
  width: 160px;
}
#login {
  background-color: #fff;
  box-shadow: 0px 3.663px 32.0513px rgba(0, 0, 0, 0.08);
  border-radius: 20px;
  padding: 40px 35px;
}
.formblk  .form-control{
	background: #FFFFFF;
	border: 1px solid #ADADAD;
	border-radius: 8px;
	padding: 16px 20px;
}
.formblk .form-label {
  margin-bottom: 12px;
}
.form-input {
  position: relative;
}
.icon_wrp {
  position: absolute;
  right: 18px;
  top: 15px;
  cursor: pointer;
}
.forgot_password{
	font-weight: 400;
	font-size: 13px;
	line-height: 15px;
	color: #8C8C8C;
	text-align: right;
}
.button_login {
	background: #C4161C;
	color: #fff;
	font-size: 17px;
	width: 100%;
	border: 0;
	border-radius: 10px;
	padding: 14px;
	line-height: 20px;
	transition: all 0.3s ease-out;
	box-shadow: 0 0.5em 0.5em -0.4em #aaa;
	display: block;
	text-align: center;
	cursor:pointer;
}
.button_login:hover{
	transform: translateY(-0.25em);
	background-color: #C4161C;
	color: #fff;
}

.button_border {
	background: #fff;
	color: #000;
	font-size: 17px;
	width: 100%;
	border: 0;
	border: 1px solid #C4161C;
	border-radius: 10px;
	padding: 14px;
	line-height: 20px;
	transition: all 0.3s ease-out;
	box-shadow: 0 0.5em 0.5em -0.4em #aaa;
	display: block;
	text-align: center;
	cursor:pointer;
}
.button_border:hover{
	transform: translateY(-0.25em);
	color: #000;
}
.settext {
  font-size: 11px;
  color: #444343;
}

/*--- Login End ----*/

/*--- Header Start ----*/
.pagetitle{
	font-weight: 700;
	font-size: 15px;
	line-height: 15px;
	text-align: center;
	text-transform: uppercase;
	color: #000000;
	padding-top: 14px;
}
.pagetitle img{
	margin-right: 10px;
}
.user_blk {
  display: inline-block;
}
.notification_blk {
  position: relative;
  vertical-align: middle;
  border-right: 1px solid #72767921;
  padding-right: 12px;
  margin-right: 24px;
  padding-top: 8px;
}
.notification_blk span {
  position: absolute;
  right: 17px;
  background-color: #C4161C;
  font-size: 8px;
  color: #fff;
  width: 12px;
  height: 12px;
  border-radius: 100%;
  text-align: center;
  font-weight: bold;
  top: 4px;
}
.user_wrp  a{
	font-weight: 400;
	font-size: 16px;
	line-height: 19px;
	color: rgba(51, 51, 51, 0.8);
}
.user_wrp  a img{
	margin-right: 10px;
}
.logout a{
	font-weight: 400;
	font-size: 16px;
	line-height: 19px;
	color: rgba(51, 51, 51, 0.8);
}
.logout {
  padding-top: 10px;
}
.notification_blk a{
	display: inline-block;
}
.user_wrp {
  margin-right: 20px;
}
header{
	width: 100%;
	background: #FFFFFF;
	box-shadow: 0px 2px 10px rgba(0, 0, 0, 0.15);
}
.user_wrp .dropdown-toggle::after {  
	margin-left: 10px;
	vertical-align: 4px;
	border-top: 0;
	border-right: 0;
	border-bottom: 0;
	border-left: 0;
	background: url(../images/arrow.png);
	width: 8px;
	height: 6px;
  
}
/*------ Header Ends -------*/
/*------ Sidebar Start -------*/
#sidebar {
  width: 210px;
  float: left;
  height: 100vh;
  position: fixed;
  background: #00346B;
  text-align: center;
  transition: all 0.4s ease-out;
}
.sidebar{
	text-align: left;
}
.logo_sidebar {
	padding: 28px 28px 18px 28px;  
	text-align: center;
}
.nav-item {
  font-weight: 400;
  font-size: 14px;
  line-height: 10px;
  color: #FFFFFF;
}
.nav-item a{
  color: #FFFFFF;
}
.nav-link {
  padding: 15px 15px 15px 25px;
}
.nav-tabs .nav-link:hover{
	border-bottom: 2px solid #fff;
}
.nav-item.active span {
  border-left: 5px solid #EE3637;
  border-radius: 7px;
  display: inline-block;
  width: 5px;
  height: 44px;
  position: absolute;
}
.nav-item a img{
  margin-right: 8px;
}
.nav-item a:hover{
  color: #FFFFFF;
}
.nav-link:hover {
  color: #fff;
}
.nav-link:hover, .nav-link:focus {
  color: #fff;
}
/*------ Main Content Start -------*/
#content_area {
	background: #F5F5F5;
	margin-left: 210px;
	overflow: visible;
	min-height: 100%;
	transition: all 0.4s ease-out;
	float: left;
	width: calc(100% - 210px);
