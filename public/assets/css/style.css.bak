.filebutton_wrp {
	background: #FFFFFF;
	border: 1px dashed #ADAABF;
	box-shadow: 0px 1px 2px rgba(50, 43, 95, 0.08);
	border-radius: 2px;
	text-align: center;
	padding: 7% 20px;
	margin-top: 24px;
}
.filebutton_wrp [type="file"] {
  height: 0;
  overflow: hidden;
  width: 0;
}
.filebutton_wrp [type="file"] + label {
  border: none;
  border-radius: 5px;
  cursor: pointer;
  display: inline-block;
  font-size: inherit;
  font-weight: 400;
  margin-bottom: 0;
  outline: none;
  position: relative;
  transition: all 0.3s;
  vertical-align: middle;
}
.filebutton_wrp label{
  font-weight: 500 !important;
  font-size: 13px !important;
  line-height: 16px;
  color: #444343;
}
.filebutton_wrp label img{
    margin: 0 auto 7px;
    text-align: center;
    display: block;
}
