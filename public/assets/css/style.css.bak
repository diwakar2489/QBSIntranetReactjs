}
.btn_style1:hover{
	color: #000;
	background-color: #ffffff;
	border: 1px solid #00346B;
}
.table_style_1 {
	border-radius: 6px;
	box-shadow: 0px 4px 10px 2px rgba(0, 52, 107, 0.2);
	background-color: #fff;
	width: 100%;
	overflow: hidden;
}
.table_style_1 thead {
	background-color: #00346B;
	font-size: 13px;
	line-height: 16px;
	color: #fff;
	text-transform: uppercase;
}
.table_style_1 thead tr {
  border-bottom: 1px solid #CECCDD;
}
.table_style_1 thead tr th{
  padding: 15px 15px;
  font-weight: normal;
}
.table_style_1 tr td {
	padding: 20px 15px;
	font-weight: 400;
	font-size: 13px;
	line-height: 16px;
	color: #666666;
}
.table_style_1 tbody tr {
  border-bottom: 1px solid #CECCDD;
}
.table_style_1 tbody tr:hover {
  background-color: #FFE1E6;
}
.active_btn, .inactive_btn {
  border-radius: 3px;
  padding: 5px 12px;
